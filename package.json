{
  "private": true,
  "packageManager": "pnpm@7.30.3",
  "scripts": {
    "dev": "ENV=dev nuxi dev",
    "build": "ENV=production nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,ts,tsx,vue,yml}\" --write --ignore-path .gitignore",
    "prepare": "simple-git-hooks && nuxi prepare"
  },
  "lint-staged": {
    "**/*.{ts,vue}": "eslint --cache --ignore-path .gitignore"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@sindresorhus/slugify": "^2.2.0",
    "@types/node": "^18.15.11",
    "@vueuse/nuxt": "^9.13.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "lint-staged": "^13.2.0",
    "medium-zoom": "^1.0.8",
    "nuxt": "^3.3.3",
    "nuxt-kql": "^0.26.3",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "sass": "^1.63.6",
    "scss": "^0.2.4",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.3",
    "vite": "^4.3.9",
    "vue-tsc": "^1.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vue"
      ]
    }
  },
  "dependencies": {
    "@nuxtjs/sentry": "^7.5.0",
    "@sentry/tracing": "^7.66.0",
    "@sentry/vue": "^7.66.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^10.2.1",
    "@vueuse/motion": "^2.0.0",
    "install": "^0.13.0",
    "mapbox-gl": "^2.15.0",
    "npm": "^10.0.0",
    "scrollreveal": "^4.0.9",
    "scrolly-video": "^0.0.16",
    "three": "^0.154.0",
    "vue-intersect": "^1.1.6",
    "vue-rough": "^1.1.0",
    "vue3-rough": "^1.1.4"
  }
}
